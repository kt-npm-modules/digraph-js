{
  "name": "ya-digraph-js",
  "description": "[yet another] A dependency free library to create and traverse directed graphs",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Konstantin Tarmyshov",
    "email": "k.tarmyshov@googlemail.com"
  },
  "keywords": [
    "node",
    "javascript",
    "typescript",
    "nodejs",
    "node_modules",
    "tree",
    "npm",
    "graph",
    "visualization",
    "dependencies",
    "lightweight"
  ],
  "homepage": "https://github.com/kt-public/ya-digraph-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kt-public/digraph-js.git"
  },
  "type": "module",
  "types": "dist/cjs/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build:esm": "tsc --project tsconfig.esm.json && tsc-alias --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json && tsc-alias --project tsconfig.cjs.json",
    "build:dual": "rimraf ./dist && npm run build:esm && npm run build:cjs && tsconfig-to-dual-package ./tsconfig.esm.json ./tsconfig.cjs.json && npm run prepack",
    "build:simple": "rimraf ./dist && tsc --project tsconfig.json && tsc-alias --project tsconfig.json && npm run prepack",
    "build": "npm run build:dual",
    "prepare": "echo ''",
    "prepack": "publint",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "check": "tsc --skipLibCheck",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "changeset:version": "changeset version && git add --all",
    "changeset:publish": "changeset publish"
  },
  "files": ["dist", "!dist/**/*.test.*", "!dist/**/*.spec.*", "!tests"],
  "bugs": {
    "url": "https://github.com/kt-public/digraph-js/issues"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/compat": "^1.2.7",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.10",
    "@vitest/coverage-istanbul": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.5.3",
    "publint": "^0.3.11",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.15",
    "tsconfig-to-dual-package": "^1.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
